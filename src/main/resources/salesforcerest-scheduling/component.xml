<component name="salesforcerest-scheduling" type="synapse/template" displayName="Scheduling">
    <subComponents>
        <component name="listSchedulerRESTResourcesAndURIs">
            <displayName>List Scheduler REST Resources And URIs</displayName>
            <file>listSchedulerRESTResourcesAndURIs.xml</file>
            <description>
                Retrieves a list of available Salesforce Scheduler REST resources and corresponding URIs.
            </description>
        </component>
        <component name="listAppointmentSlots">
            <displayName>List Appointment Slots</displayName>
            <file>listAppointmentSlots.xml</file>
            <description>
                Retrieves available appointment slots by work type and service territories.
            </description>
        </component>
        <component name="listAppointmentCandidates">
            <displayName>List Appointment Candidates</displayName>
            <file>listAppointmentCandidates.xml</file>
            <description>
                Retrieves service resources based on work type and service territories.
            </description>
        </component>
    </subComponents>
</component>

